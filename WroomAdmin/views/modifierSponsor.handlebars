<h1>Modifier un Sponsor</h1>

<form id="loginformA" action="FinirModifierSponsor" method="post">

  <div>
      <label for="login">Nom: </label>
      <input type="text" id="login" name="nom" value="{{sponsor.SPONOM}}"></input>
  </div>
  <div>
      <label for="login">Secteur d'activite: </label>
      <input type="text" id="login" name="seactivite" value="{{sponsor.SPOSECTACTIVITE}}"></input>
  </div>

  <div>
      <label for="login">Ecurie sponsorisee: </label>
      <select id="nationalite-select" name="ecurie">
        <option value=null> Aucune </option>
        {{#each listeEcurie}}

        <option value="{{this.ecunum}}" selected="{{sponsor.ECUNOM}}"> {{this.ecunom}}</option>
        {{/each}}
      </select>
  </div>

  <input id="prodId" name="num" type="hidden" value="{{sponsor.SPONUM}}">

<input type="submit" value="AJOUTER"> </input>
</form>
